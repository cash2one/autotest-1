{% extends "base.html" %}
{% load i18n %}
{% load url from future %}
{% load platFilter %}

{% block content %}
   <legend><i class="icon-th-list"></i>历史任务</legend>
   <div class="bs-docs-example">
            <ul id="myTab" class="nav nav-tabs">
              <li class="active"><a href="#taskHistoryTab" data-toggle="tab" name="1">线下直销</a></li>
              <li><a href="#taskHistoryTab" data-toggle="tab" name="2">词典</a></li>
              <li><a href="#taskHistoryTab" data-toggle="tab" name="5">DSP</a></li>
            </ul>
            <div id="myTabContent" class="tab-content">
              <div class="tab-pane fade in active" id="offline">
                <table class="table table-bordered table-striped table-hover">
                    <thead>
                    <tr>
                        <th scope="col" class="sortable">
                            <div class="dropdown pull-left"> <a class="dropdown-toggle" data-toggle="dropdown" >时间 </a></div>
                        </th>
                        <th scope="col" class="sortable">
                            <div class="dropdown pull-left"> <a class="dropdown-toggle" data-toggle="dropdown" >代码版本 </a></div>
                        </th>
                        <th scope="col" class="sortable">
                            <div class="dropdown pull-left"> <a class="dropdown-toggle" data-toggle="dropdown" >结果 </a></div>
                        </th>
                        <th scope="col" class="sortable"> 
                            <div class="dropdown pull-left"> <a class="dropdown-toggle" data-toggle="dropdown" >测试报告 </a></div>
                        </th>
                        <th scope="col" class="sortable">
                            <div class="dropdown pull-left"> <a class="dropdown-toggle" data-toggle="dropdown" >执行人 </a></div>
                        </th>
                    </tr>
                    </thead>
                    <tbody>
                        {% for task in taskList  %}
                        <tr>
                            <td>{{task.TIME}}</td>
                            <td>{{task.ENV_ID.IMPR_SVN}}</td>
                            <td>{{ taskResult|key:task.RESULT}}</td>
                            <td><a href="../task_report?taskId={{task.id}}">查看</a></td>
                            <td>{{task.LDAP}}</td>
                        </tr>
                        {% endfor%}
                    </tbody>
                    </table>
                 </div> 
            </div>
            <div id="taskTable">
            </div>
   </div>
   <br>
   <br>
   <br>
   <br>
   <br>
   <br>
{% endblock%}

    {% block breadcrumbs %}
        <ul class="breadcrumb">
        <li><a href="">{% trans 'Home' %}</a>
            <span class="divider">/</span>
        <li>
        <li class="active">new_task</li>
        </ul>
    

    
    {% endblock %}
